<Application x:Class="RPA_Workbench.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:ribbon="http://schemas.actiprosoftware.com/winfx/xaml/ribbon"
             xmlns:shared="http://schemas.actiprosoftware.com/winfx/xaml/shared"
             xmlns:themes="http://schemas.actiprosoftware.com/winfx/xaml/themes"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:RPA_Workbench"
             ShutdownMode="OnMainWindowClose"
             >
    <Application.Resources>
        <SolidColorBrush x:Key="{x:Static themes:AssetResourceKeys.RibbonTabItemForegroundNormalBrushKey}" Color="White" />
        <SolidColorBrush x:Key="{x:Static themes:AssetResourceKeys.RibbonContextualTabItemForegroundHoverBrushKey}" Color="Black" />
        <SolidColorBrush x:Key="{x:Static themes:AssetResourceKeys.RibbonContextualTabItemForegroundNormalBrushKey}" Color="White" />
        <SolidColorBrush x:Key="{x:Static themes:AssetResourceKeys.RibbonTabSeparatorBackgroundNormalBrushKey}" Color="White" />
        <SolidColorBrush x:Key="{x:Static themes:AssetResourceKeys.RibbonTabItemForegroundHoverBrushKey}" Color="Black" />
       
        <!--<SolidColorBrush x:Key="{x:Static themes:AssetResourceKeys.RibbonTabSeparatorBackgroundNormalBrushKey}" Color="Red" />-->

 
    </Application.Resources>
</Application>
